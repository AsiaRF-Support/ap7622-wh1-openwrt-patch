diff --git a/target/linux/bcm27xx/image/Makefile b/target/linux/bcm27xx/image/Makefile
--- a/target/linux/bcm27xx/image/Makefile
+++ b/target/linux/bcm27xx/image/Makefile
@@ -65,6 +65,12 @@ define Build/boot-ekh01
 
 endef
 
+define Build/boot-mm610x
+    echo -e '\ndtoverlay=sysinfo,board-name="$(SYSINFO_BOARD_NAME)",model="$(SYSINFO_MODEL)"' | cat boards/mm610x-h06/distroconfig.txt boards/mm610x-h06/distroconfig-$(DISTROCONFIG_EXTRA).txt - | mcopy -o -i $@.boot - ::/distroconfig.txt
+
+endef
+
+
 define Build/sdcard-img
 	./gen_rpi_sdcard_img.sh $@ $@.boot $(IMAGE_ROOTFS) \
 		$(CONFIG_TARGET_KERNEL_PARTSIZE) $(CONFIG_TARGET_ROOTFS_PARTSIZE)
@@ -219,6 +225,21 @@ define Device/morse_ekh01_base
   IMAGE/factory.img.gz := boot-common | boot-2711 | boot-ekh01 | sdcard-img | gzip
 endef
 
+define Device/asiarf_mm610x_base
+  $(Device/rpi-4)
+  SYSINFO_BOARD_NAME := $(subst _,$(comma),$(1))
+  SUPPORTED_DEVICES := $$(SYSINFO_BOARD_NAME)
+  DEVICE_VENDOR := AsiaRF
+  # remove the factory image (causes confusion)
+  IMAGES := sysupgrade.img.gz
+  # Simplify by removing board and OpenWRT version.
+  IMAGE_PREFIX = $$(VERSION_DIST_SANITIZED)-$$(IMG_PREFIX_VERCODE)$$(IMG_PREFIX_EXTRA)$$(call sanitize,$$(DEVICE_MODEL)-$$(DEVICE_VARIANT))
+  DEVICE_IMG_NAME = $$(IMAGE_PREFIX)-$$(1)-$$(2)
+  DEVICE_PACKAGES += kmod-morse netifd-morse morse-fw-6108
+  IMAGE/sysupgrade.img.gz := boot-common | boot-2711 | boot-mm610x | sdcard-img | gzip | append-metadata
+  IMAGE/factory.img.gz := boot-common | boot-2711 | boot-mm610x | sdcard-img | gzip
+endef
+
 define Device/morse_mmx108-ekh01-sdio
   $(Device/morse_ekh01_base)
   DEVICE_MODEL := MMx108-EKH01
@@ -261,4 +282,17 @@ ifeq ($(SUBTARGET),bcm2711)
   TARGET_DEVICES += morse_mm6108-ekh01-spi
 endif
 
+define Device/asiarf_mm610x-h06-spi-and-sdio
+  $(Device/asiarf_mm610x_base)
+  DEVICE_MODEL := MM610X-H06
+  DEVICE_VARIANT := SPI and SDIO
+  SYSINFO_MODEL := $$(DEVICE_VENDOR) $$(DEVICE_MODEL) ($$(DEVICE_VARIANT))
+  SUPPORTED_DEVICES += asiarf,mm610x-h06
+  DISTROCONFIG_EXTRA := mm610x-spi-and-sdio
+endef
+ifeq ($(SUBTARGET),bcm2711)
+  TARGET_DEVICES += asiarf_mm610x-h06-spi-and-sdio
+endif
+
+
 $(eval $(call BuildImage))
